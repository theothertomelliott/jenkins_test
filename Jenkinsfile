    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                sh 'ls'
                sh 'pwd'
            }
        }
        stage('Test') {
            steps {
                echo 'Test test..'
            }
        }
        stage('Test2') {
            steps {
                echo 'Test test..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying once again....'
            }
        }
    }
}